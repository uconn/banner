{
    "name": "uconn-banner",
    "version": "2.0.2",
    "description": "uconn-banner is the official UConn Banner module packaged for quick implementation.",
    "license": "UNLICENSED",
    "repository": {
        "type": "git",
        "url": "https://github.com/uconn/banner.git"
    },
    "keywords": [
        "uconn",
        "banner"
    ],
    "bugs": {
        "url": "https://github.com/uconn/banner/issues"
    },
    "scripts": {
        "banner:sc-basic": "php ./src/Banner/Generator.php -p ./_includes/sc-basic.html -v true -s \"University Communications\" -a \"UComm\" -u https://communications.uconn.edu",
        "banner:sc-advanced": "php ./src/Banner/Generator.php -p ./_includes/sc-advanced.html -v true -s \"University Communications\" -a \"UComm\" -u https://communications.uconn.edu -m=true --a-z-dropdown=true",
        "js:uglify": "uglifyjs js/banner.js --source-map --output _site/js/banner.js -c -m",
        "js:watch": "chokidar \"js/banner.js\" -c \"npm run js:uglify\"",
        "sass:build": "compass clean && compass compile",
        "sass:watch": "compass watch &",
        "jekyll:build": "bundle exec jekyll build",
        "jekyll:watch": "bundle exec jekyll serve --watch",
        "banners": "npm run banner:sc-basic npm run banner:sc-advanced",
        "build": "npm run jekyll:build && npm run sass:build && npm run js:uglify",
        "develop": "npm run banners && npm-run-all --parallel sass:watch js:watch jekyll:watch",
        "watch": "npm run develop",
        "start": "npm run build"
    },
    "contributors": [
        {
            "name": "Brian Daley"
        },
        {
            "name": "Brian Kelleher"
        }
    ],
    "directories": {},
    "devDependencies": {
        "chokidar-cli": "^2.1.0",
        "npm-run-all": "^4.1.5",
        "uglify-js": "^3.7.2"
    },
    "dependencies": {}
}
